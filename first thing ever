var name = 'Kevin';
let age = 34;
const gender = 'male';
let life;
console.log(`My name is ${name}, I'm ${age}, and I'm a ${gender} person.`);
console.log('What is life?' + life);

console.log('What is time?' + typeof age);
console.log('10 years passed');

age += 10;
console.log(`My name is ${name}, I'm ${age}, and I'm a ${gender} person.`);
